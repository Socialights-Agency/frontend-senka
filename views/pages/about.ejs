<!DOCTYPE html>
<html lang="en">

<head>
    <%- include('../partials/head'); %>
</head>

<body id="aboutPage">
    <header>
        <%- include('../partials/nav'); %>
    </header>

    <section class="banner-jumbo">
        <img src="<%-banner.banner_desk%>" class="lazy img-fluid bg-about" alt="background">
        <img src="<%-banner.banner_mbl%>" class="lazy img-fluid bg-about-mobile" alt="background">
        <div class="container-fluid">
            <div class="box-about">
                <div class="title" data-aos="fade-up" data-aos-easing="linear" data-aos-duration="600">
                    <h1><%-banner.title%></h1>
                </div>
                <div class="desc-box" data-aos="fade-up" data-aos-easing="linear" data-aos-duration="600"
                    data-aos-delay="300">
                    <p class="text">
                        <%-banner.subtitle%>
                    </p>
                </div>
            </div>
        </div>
    </section>

    <section class="section-one">
        <img class="img-fluid icon-foam" src="./assets/images/about/icon-whip.webp" alt="icon">
        <div class="container">
            <div class="box-about">
                <div class="title" data-aos="fade-up" data-aos-easing="linear" data-aos-duration="600">
                    <h1><%-content.title_sec_1%></h1>
                </div>
                <div class="desc-box" data-aos="fade-up" data-aos-easing="linear" data-aos-duration="600"
                    data-aos-delay="300">
                    <p class="text">
                        <%-content.desc_sec_1%>
                    </p>
                </div>
            </div>
            <div class="row box-one">
                <div class="col-12">
                    <div class="box-img" data-aos="fade-up" data-aos-easing="linear" data-aos-duration="400"
                        data-aos-delay="400">
                        <img class="img-fluid" src="<%-content.img_sec_1%>" alt="image-one">
                    </div>
                </div>
                <div class="col-12">
                    <div class="box-desc" data-aos="fade-up" data-aos-easing="linear" data-aos-duration="400"
                        data-aos-delay="500">
                        <div class="title">
                            <h5><%-content.title_sec_1%></h5>
                        </div>
                        <div class="desc">
                            <p class="text">
                                <%-content.desc_sec_1%>
                            </p>
                        </div>
                    </div>
                </div>
            </div>
            <div class="row box-two">
                <div class="col-12 col-sm-7">
                    <div class="box-img" data-aos="fade-up" data-aos-easing="linear" data-aos-duration="400"
                        data-aos-delay="600">
                        <img class="img-fluid" src="<%-content.img_sec_2%>" alt="image-one">
                    </div>
                </div>
                <div class="col-12 col-sm-5">
                    <div class="box-desc" data-aos="fade-up" data-aos-easing="linear" data-aos-duration="400"
                        data-aos-delay="700">
                        <div class="title">
                            <h5><%-content.title_sec_2%></h5>
                        </div>
                        <div class="desc">
                            <p class="text">
                                <%-content.desc_sec_2%>
                            </p>
                        </div>
                    </div>
                </div>
            </div>
            <div class="row box-three">
                <div class="col-12">
                    <div class="box-img" data-aos="fade-up" data-aos-easing="linear" data-aos-duration="400"
                        data-aos-delay="800">
                        <img class="img-fluid" src="<%-content.img_sec_3%>" alt="image-one">
                    </div>
                </div>
                <div class="col-12">
                    <div class="box-desc" data-aos="fade-up" data-aos-easing="linear" data-aos-duration="400"
                        data-aos-delay="900">
                        <div class="title">
                            <h5><%-content.title_sec_3%></h5>
                        </div>
                        <div class="desc">
                            <p class="text">
                                <%-content.desc_sec_3%>
                            </p>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </section>

    <footer>
        <%- include('../partials/footer'); %>
    </footer>
    <%- include('../partials/script'); %>
</body>

</html>