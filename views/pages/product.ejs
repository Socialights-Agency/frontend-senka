        <img src="<%-banner.banner_mbl%>" class="lazy img-fluid bg-product-mobile" alt="background">
        <div class="container-fluid">
            <div class="box-product">
                <div class="title" data-aos="fade-up" data-aos-easing="linear" data-aos-duration="200">
                    <h1><%-banner.title%></h1>
                </div>
                <div class="desc-box" data-aos="fade-up" data-aos-easing="linear" data-aos-duration="400"
                    data-aos-delay="300">
                    <p class="text">
                        <%-banner.subtitle%>
                    </p>
                </div>
            </div>
        </div>
    </section>

