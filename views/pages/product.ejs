                    <% if(product && product.length > 0) { %>
                    <% product.forEach((pc, pcIndex) => { %>
                    <button class="nav-link <%-pcIndex === 0 ? 'active' : ''%>" data-bs-toggle="pill"
                        data-bs-target="#<%-pc.id%>" type="button" role="tab" aria-controls="<%-pc.id%>"
                        aria-selected="<%-pcIndex === 0 ? 'true' : 'false'%>">
                        <img src="<%-pc.thumbnail%>" class="lazy img-fluid ic-category" alt="<%-pc.name%>">
                    </button>
                    <% }) %>
                    <% } %>
