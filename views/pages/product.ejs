                                    <div class="box-square">
                                        <div class="box-img">
                                            <img src="<%-p.thumbnail%>" class="lazy img-fluid product" alt="product">

                                            <% if(p.label == 'favorite') {%>
                                            <img src="./assets/images/product/icon-first.png"
                                                class="lazy img-fluid attribute" alt="attribute">
                                            <% } else if(p.label == 'new') {%>
                                            <img src="./assets/images/product/icon-new.png"
                                                class="lazy img-fluid attribute" alt="attribute">
                                            <% } %>
                                            <div class="overlay">
                                                <% if(p.texture_img) {%>
                                                <img src="<%-p.texture_img%>" class="lazy img-fluid hoverlay"
                                                    alt="icon">
                                                <% } else { %>
                                                <img src="<%-p.thumbnail%>" class="lazy img-fluid hoverlay" alt="icon">
                                                <% }%>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="box-title">
                                        <h5 class="text-title">
                                            <%-p.name%>
                                        </h5>
                                    </div>
                                </a>
                            </div>
                            <% }) %>
